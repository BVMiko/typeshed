from authlib.oauth2.rfc6749 import JsonRequest, OAuth2Request
from django.http import HttpRequest as HttpRequest

class DjangoOAuth2Request(OAuth2Request):
    def __init__(self, request: HttpRequest) -> None: ...
    @property
    def args(self): ...
    @property
    def form(self): ...
    def data(self): ...
    def datalist(self): ...

class DjangoJsonRequest(JsonRequest):
    def __init__(self, request: HttpRequest) -> None: ...
    def data(self): ...
