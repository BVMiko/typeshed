from functools import cached_property as cached_property

from authlib.oauth2.rfc6749 import JsonRequest, OAuth2Request
from flask.wrappers import Request as Request

class FlaskOAuth2Request(OAuth2Request):
    def __init__(self, request: Request) -> None: ...
    @property
    def args(self): ...
    @property
    def form(self): ...
    @property
    def data(self) -> dict[str, str]: ...
    @cached_property
    def datalist(self) -> dict[str, list]: ...

class FlaskJsonRequest(JsonRequest):
    def __init__(self, request: Request) -> None: ...
    @property
    def data(self): ...
