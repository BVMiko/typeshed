class ClientMixin:
    def get_client_id(self) -> None: ...
    def get_default_redirect_uri(self) -> None: ...
    def get_allowed_scope(self, scope) -> None: ...
    def check_redirect_uri(self, redirect_uri) -> None: ...
    def check_client_secret(self, client_secret) -> None: ...
    def check_endpoint_auth_method(self, method, endpoint) -> None: ...
    def check_token_endpoint_auth_method(self, method): ...
    def check_response_type(self, response_type) -> None: ...
    def check_grant_type(self, grant_type) -> None: ...

class AuthorizationCodeMixin:
    def get_redirect_uri(self) -> None: ...
    def get_scope(self) -> None: ...

class TokenMixin:
    def check_client(self, client) -> None: ...
    def get_scope(self) -> None: ...
    def get_expires_in(self) -> None: ...
    def is_expired(self) -> None: ...
    def is_revoked(self) -> None: ...
